@{
    ViewData["Title"] = SharedResource["Employee Tranfer"];
}
<div class="card">
    <div class="card-body">
        <div class="mb-3">
            <button id="btn-add" class="btn btn-info" onclick="showEmpTransferModal('')">New Employee Transfer</button>
        </div>
        <table class="table-sm table" id="EmployeeTransferDataTable">
            <thead class="thead-dark">
                <tr>
                    <th>No.</th>
                    <th>Employee</th>
                    <th>Tran Dt</th>
                    <th>Dept Fr</th>
                    <th>Dept To</th>
                    <th>Loc Fr</th>
                    <th>Loc To</th>
                    <th>Narration</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<div id="EmployeeTransferModal" class="modal fade" role="dialog" tabindex='-1'></div>
@section scripts {
    <script src="~/js/page/transactions/emp-transfer.js" asp-append-version="true"></script>
}
