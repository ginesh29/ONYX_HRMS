@model dynamic
@{
    var excelData = (IEnumerable<EmpAttendance_Getrow_Result>)Model.Data;
}
<table class="table table-sm mt-3" id="ExecelData">
    <thead class="thead-dark">
        <tr>
            <th>Employee Code</th>
            <th>No Of Days</th>
            <th>Paid</th>
            <th>Unpaid</th>
            <th>W.OT</th>
            <th>H.OT</th>
        </tr>
    </thead>
    @foreach (var item in excelData)
    {
        <tr>
            <td>
                <span>
                    @item.Cd
                    @if (item.IsValid)
                    {
                        <i class="far fa-check-circle text-success" aria-hidden="true"></i>
                    }
                    else
                    {
                        <i class="far fa-times-circle text-danger" aria-hidden="true" data-toggle="tooltip" data-original-title="@item.ErrorMessage"></i>
                    }
                </span>
            </td>
            <td>@item.W_days</td>
            <td>@item.P_HDays</td>
            <td>@item.Up_HDays</td>
            <td>@item.W_OT</td>
            <td>@item.H_OT</td>
        </tr>
    }
</table>
<script>
    if ('@Model.Message'.includes("succussfully"))
        showSuccessToastr('@Model.Message');
    else
        showErrorToastr('@Model.Message');
</script>