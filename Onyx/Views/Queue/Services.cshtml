@{
    ViewData["Title"] = SharedResource["Services"];
}
<div class="card">
    <div class="card-body">
        <div class="mb-3">
            <button class="btn btn-info" onclick="showServiceModal('')">New Service</button>
        </div>
        <table class="table table-sm" id="ServicesDataTable">
            <thead>
                <tr class="thead-dark">
                    <th>No.</th>
                    <th>Code</th>
                    <th>Prefix</th>
                    <th>Service Name</th>
                    <th>Active</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<div id="ServiceModal" class="modal fade" role="dialog" tabindex='-1'></div>
@section scripts {
    <script src="~/js/page/queue/service.js" asp-append-version="true"></script>
}