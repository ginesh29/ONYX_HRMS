@model TokenModel
<div class="modal-dialog modal-sm">
    <!-- Modal content-->
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title">Generate Token</h4>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
            <form id="token-frm">
                <div class="form-group">
                    <label asp-for="ServiceCd"></label>
                    <select asp-for="ServiceCd" type="text" class="form-control select-picker" asp-items="@ViewBag.Services"></select>
                    <span asp-validation-for="ServiceCd" class="text-danger"></span>
                </div>
                @* <div class="form-group">
                    <label asp-for="MobileNo"></label>
                    <input asp-for="MobileNo" type="text" class="form-control" />
                </div> *@
            </form>
            <div id="token-container"></div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-info" onclick="saveToken(this)" id="btn-submit">Submit</button>
            <button type="button" class="btn btn-warning d-none" onclick="printToken(this)" id="btn-print">Print</button>
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>