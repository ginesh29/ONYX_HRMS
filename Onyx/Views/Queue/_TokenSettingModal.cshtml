@model TokenSettingModel
<div class="modal-dialog modal-md">
    <!-- Modal content-->
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title">Token Setting</h4>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
            <form id="token-setting-frm">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label asp-for="ServiceCd"></label>
                            <select asp-for="ServiceCd" type="text" class="form-control select-picker" asp-items="ViewBag.ServiceItems"></select>
                            <span asp-validation-for="ServiceCd" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label asp-for="CounterName"></label>
                            <select asp-for="CounterName" type="text" class="form-control select-picker" asp-items="ViewBag.CounterItems"></select>
                            <span asp-validation-for="CounterName" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label asp-for="VoiceName"></label>
                    <select asp-for="VoiceName" type="text" class="form-control select-picker" data-size="6"></select>
                    <span asp-validation-for="VoiceName" class="text-danger"></span>
                </div>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-info" onclick="saveTokenSetting(this)">Submit</button>
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>