@model dynamic
@{
    IEnumerable<EmpLeaveMaster_GetRow_Result> data = Model.Data;
    ViewData["Title"] = Model.ReportType == "LeaveMaster" ? "Employee Leave Master Report" : "Employee Annual Leave Due Report";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_HeadImports" />
</head>
<body>
    <h3 class="text-center mb-5">@ViewData["Title"]</h3>
    <h6 class="text-right"><span class="font-weight-bold">Print Date & Time:</span> @DateTime.Now</h6>
    <partial name="_LeaveMaster" model="Model" />
</body>
</html>