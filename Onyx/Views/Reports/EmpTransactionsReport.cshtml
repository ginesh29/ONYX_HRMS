@model EmpTransactionModel
@{
    var start = Context.Request.Query["StartPeriod"];
    var end = Context.Request.Query["EndPeriod"];
    ViewData["Title"] = SharedResource[$"Employee Transactions Report {start} to {end}"];
    var detail = Model.Header;
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_HeadImports" />
</head>
<body>
    <partial name="_ReportHeader" />
    <table style="width:100%">
        <tr>
            <td>
                <h6><span class="font-weight-bold">Employee:</span> @detail.EmpName (@detail.Cd.Trim())</h6>
            </td>
        </tr>
        <tr>
            <td>
                <h6><span class="font-weight-bold">Branch:</span> @detail.Branch</h6>
            </td>
            <td class="text-right"><h6><span class="font-weight-bold">DOJ:</span> @detail.DOJ.FormatDate()</h6></td>
        </tr>
    </table>
    <partial name="_EmpTransactions" model="Model" />
</body>
</html>