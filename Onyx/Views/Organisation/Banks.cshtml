@{
    ViewData["Title"] = "Banks";
}
<div class="card">
    <div class="card-body">
        <div class="mb-3">
            <button class="btn btn-info" onclick="showBankModal('')">New Bank</button>
        </div>
        <table class="table table-sm" id="BanksDataTable">
            <thead class="thead-dark">
                <tr>
                    <th>No.</th>
                    <th>Code</th>
                    <th>Image</th>
                    <th>Name</th>
                    <th>Description</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<div id="BankModal" class="modal fade" role="dialog" tabindex='-1'></div>
@section scripts {
    <script src="~/js/page/bank.js" asp-append-version="true"></script>
}