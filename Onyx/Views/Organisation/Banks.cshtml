@{
    ViewData["Title"] = "Banks";
}
<div class="card">
    <div class="card-body">
        <div class="mb-3">
            <button id="btn-add" class="btn btn-info" onclick="showBankModal('')">New Bank</button>
        </div>
        <div class="table-responsive">
            <table class="table-sm table" id="BanksDataTable">
                <thead class="thead-dark">
                    <tr>
                        <th>No.</th>
                        <th>Bank</th>
                        <th>Branch</th>
                        <th>Swift Code</th>
                        <th>Address 1</th>
                        <th>Address 2</th>
                        <th>Address 3</th>
                        <th>Contact</th>
                        <th>Phone</th>
                        <th>Fax</th>
                        <th>Email</th>
                        <th>Website</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
<div id="BankModal" class="modal fade" role="dialog" tabindex='-1'></div>
@section scripts {
    <script src="~/js/page/organisation/bank.js" asp-append-version="true"></script>
}